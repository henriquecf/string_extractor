<%= form_for @ticketed_event, html: {id: "donate_form", class: "create_donate_form js-create_donate_form"} do |f| %>

    <%= hidden_field_tag :current_step, "edit" %>
    <div id="sidebar" class="donate_sidebar js-donate_sidebar js-sidebar">
        <div class="action_widget action_status_tracker clearfix">
            <div id="goal_tracker" class="pb10 js-goal_tracker">
                <div class="action_status_running_total">
                    <span class="relative block">
						<%= f.select :slider_type, [["Number of Tickets", "ticket_slider-tickets"], ["Dollars Raised", "ticket_slider-dollars"]], {},  { class: "can_select ticketed_slider_type slider_type js-ticketed_slider_type" } %>

                        <a class="tooltip right js-tooltip">?<span>You can count number of tickets sold or total dollars raised, or you can remove the goal section entirely. Goals are autogenerated.</span></a>
                    </span>
                </div>
                <div class="action_status_status_bar"><span style="width:50%;"><span class="action_status_status_bar-grow js-action_status_status_bar-grow"></span></div>
                <small class="block centertext mt-10 graytext ticketed_slider_preview-tickets js-ticketed_slider_preview-tickets">Only <strong>250</strong> more tickets until our goal of 500 tickets</small>
                <small class="block centertext mt-10 graytext ticketed_slider_preview-dollars js-ticketed_slider_preview-dollars">Only <strong>$50,000</strong> more until our goal of $100,000</small>
            </div>
            <label id="disable_goal_counter" class="button button_green js-disable_goal_counter">
            <%= f.radio_button :show_goal, false %>
            Remove Goal Slider</label>
            <label id="enable_goal_counter" class="button button_green js-enable_goal_counter">
            <%= f.radio_button :show_goal, true %>
            Add Goal Slider</label>
        </div>
        <div class="sticky_sidebar js-sticky_sidebar action_sidebar">
	        <div id="action_info" class="noborder">
				<div id="action_info_inner" class="clearfix">
					<div class="sponsor_owner_box clearfix">
						<div id="creator_box" class="clearfix js-creator_box">
							<div class="action_owner_title nopt"><h6>Created by</h6></div>
							<div class="action_owner_wrap">
								<div class="action_owner">
									<a href="<%=current_user ? "/users/#{@ticketed_event.user.permalink}/profile" : "#"%>">
										<%=  user_avatar(@ticketed_event.user, :medium, "action_author_avatar avatar").html_safe %>
                					</a>
									<div class="action_info_user js-action_info_user">
										<a href="<%=current_user ? "/users/#{@ticketed_event.user.permalink}/profile" : "#"%>">
											<%= @ticketed_event.user.first_name %> <%= @ticketed_event.user.last_name %>
                  						</a>
                					</div>
									<% if @ticketed_event.user.zip_code %>
									<div class="action_info_location"><%= @ticketed_event.user.zip_code %></div>
									<% end %>
              					</div>
            				</div>
							 <% if current_user and current_user.in_groups? %>
                            <a href="javascript:void(0);" class="remove_owner js-remove_owner left button mini_button mt10 hundred-40">Remove Creator</a>
							<a class="tooltip js-tooltip left ml10 mt5">?<span>If you'd rather not put your name on this action you can remove the creator section entirely. If you do this, you must specify a sponsoring group for this action below. People who take action will be added to the sponsoring group's list but not your individual list.</span></a>
							<%end%>
          				</div>

		  			<div id="no_creator_box" class="js-no_creator_box">
		  				<div class="action_owner_title nopt"><h6 class="disabled">Created by</h6></div>
		  				<div class="clearfix">
			  				<a href="javascript:void(0);" class="add_owner js-add_owner left button mini_button mt5 hundred-40">Add Creator</a>
			  				<a class="tooltip js-tooltip left ml10">?<span>You can put your name on this action as the action creator for added authenticity. In addition, people who sign up will be added to your individual list as well as the sponsoring group's list.</span></a>
		  				</div>
		  				
		  				<% if ENV['RAILS_ENV'] == 'development' %>
			            <div class="small hundred-40 centertext">or</div>
			            <div class="clearfix">
					        <input type="text" class="hundred-40 js-custom_creator" name="custom_creator" value="" placeholder="Add Custom Name" />
				            <a class="tooltip js-tooltip ml10 mt5">?<span>You can use any text as your creator name. Location will not be displayed.</span></a>
			            </div>
			            <% end %>
			            
          			</div>
					<%= f.hidden_field :display_creator, class: "js-creator_toggle", id: "creator_toggle" %>
		  			          		</div>
          
		  		 <% if current_user and current_user.in_groups? %>
		  		<div class="sponsor_owner_box clearfix">
		  			<div class="action_owner_title action_sponsor_title">
			  			<h6>Sponsored by <a class="tooltip js-tooltip ml10 mt5">?<span>If you'd like to create this ticketed event under the banner of an Action Network group you're a member of, indicate that group here.</span></a></h6>
			  		</div>
			  		<div class="action_owner_wrap action_sponsor_wrap">
			  			<div class="action_owner action_owner-select js-action_owner-select">
			  				<% groups_array = @groups.sort_by{|group| group.admin_title.to_s.downcase}.delete_if{|group|group.parent_id && group.is_leader?(current_user)}.map { |group| [group.admin_title, group.id, {class: group.current_page_wrapper_id}] } %>
			  				<div class="left relative">
			  					<%= f.select :group_id, options_for_select(groups_array.insert(0, ["--",""]),params[:group_id]||@ticketed_event.group_id)+("<optgroup label='#{current_user.network(true).name} Network'>".html_safe+options_for_select(current_user.network_groups.collect{|d| [d.admin_title||"me", d.id, {class: d.current_page_wrapper_id.to_s+" spacer-"+d.relative_network_level.to_s+" dropspacer"}]},(params[:group_id]||@ticketed_event.group_id)) if current_user.network)+"</optgroup>".html_safe,{}, {:id=>"fundraising-group-select", :class=>"js-sponsor-group-select js-fundraising-group-select"} %>
                 			</div>
                 
              			</div>
            		</div>
          		</div>
		  		<% elsif @ticketed_event.group %>
        <div class="sponsor_owner_box">
	      <input name="ticketed_event[group_id]" type="hidden" value="<%= @ticketed_event.group_id %>" />
          <div class="action_owner_title action_sponsor_title"><h6>Sponsored by</h6></div>
          <div class="action_owner_wrap action_sponsor_wrap">
            <div class="action_owner">
              <a href="/groups/<%=@ticketed_event.group.permalink%>">
                <%= image_tag(@ticketed_event.group.photo(:thumb), :class => 'action_author_avatar avatar') %>
              </a>
              <div class="action_info_user js-action_info_user"><a href="/groups/<%=@ticketed_event.group.permalink%>"><%= @ticketed_event.group.name %></a></div>
              <%if !@ticketed_event.group.zip_code.blank?%><div class="action_info_location"><%= @ticketed_event.group.city %>,<%= STATE_FULL[@ticketed_event.group.state] %></div><%end%>              </div>
          </div>
        </div>
        <% end %>
          
		  		<div class="clear"></div>
            </div>
        </div>
    </div>
    </div>
                        
    <div id="action_head" class="main_col js-main_col js-action_head">
        <div class="entry-title mb10 clearfix js-entry-title ">
            <%= f.text_field :title, id: "event-title", class: "create_action-editable js-create_action-editable required js-action_title", placeholder: "Title of your ticketed event *" %>
            <a class="tooltip js-tooltip left ml15">?<span>The shorter and more descriptive, the better.</span></a>
        </div>
				<div class="event_check_toggles pl30 mb5 mt-5 clearfix">
					<label class="small12 mr40"><%= f.check_box :has_end_date,id:"event_endtime_toggle", class:"js-event_endtime_toggle js-event_endtime_toggle-#{@ticketed_event.has_end_date ? "enabled" : "disabled"} mr5" %> This Event Has An <strong>End Time</strong></label>
					<label class="small12"><%= f.check_box :has_no_location, id: "event_physical_locatioin_toggle", class: "js-event_physical_location_toggle js-event_physical_location_toggle-#{@ticketed_event.has_no_location ? "enabled" : "disabled"} mr5" %> This Event Has No <strong>Physical Location</strong></label>
				</div>
        <div class="event_info clearfix js-event_info">
            <div class="event_date clearfix">
                <%= f.text_field :start_at, id: "event-start-date", class: "create_action-editable datepick required js-create_action-editable js-datepick js-event-start-date #{"error_input" if params[:validate] and @ticketed_event.start_at and @ticketed_event.start_at < Date.today-1}", value: (@ticketed_event.start_at.strftime("%b %d, %Y - %l:%M %p") if @ticketed_event.start_at), placeholder: "Date and Time *", size: "35" %>
            </div>
				    <div class="event_date js-event_end_date hide clearfix">
				        <%= f.text_field :end_at, :id=>"event-end-date", :class=>"js-event-end-date-input create_action-editable js-create_action-editable datepick js-datepick #{"error_input" if params[:validate] and @ticketed_event.has_end_date and @ticketed_event.end_at.to_i < @ticketed_event.start_at.to_i}", :placeholder=>"End Date and Time *", :value => (@ticketed_event.end_at.strftime("%b %d, %Y - %l:%M %p") if @ticketed_event.end_at)%>
				    </div>
            <div class="event_location clearfix js-event_location">
                <%= f.text_field :location_name, id: "event-location", class: "create_action-editable required js-create_action-editable js-event-location", placeholder: "Location Name *", size: "35" %>
                <%= f.text_field :address, id: "event-address", class: "create_action-editable required js-create_action-editable js-event-address", placeholder: "Address *", size: "35" %>
                <a class="tooltip js-tooltip left ml5">?<span>The name of the venue where you're holding the ticketed event (ex: "My House" or "Upper Senate Park"), and the address where the ticketed event is located. Please use a real address that's valid using Google Maps -- addresses like "The corner of Main and Washington" often show up incorrectly on the map we create.</span></a>
                <%= f.text_field :city, id: "event-city", class: "create_action-editable required js-create_action-editable js-event-city", placeholder: "City *", size: "35" %>
                <div id="event-state-select-wrap" class="js-event-state-select-wrap">
                    <span id="create-event-state-drop_down" class="block relative js-create-event-state-drop_down">
                        <%= f.select :state, us_states, {}, {id: "event-state", class: "required can_select js-event-state"} %>
                    </span>
                    <span class="international_link-wrap js-international_link-wrap">
                        <a class="international_link js-international_link">Not in the US?</a>
                    </span>
                    <span class="country_drop_wrap js-country_drop_wrap clear">
                        <span class="block clear relative">
                            <%= f.select :country, countries, {}, {:class => 'required can_select js-form-country', :id => 'form-country'} %>
                        </span>
                    </span>
                </div>
                <%= f.text_field :zip_code, class: "create_action-editable required js-create_action-editable js-event-zip", id: "event-zip", size: "35", maxlength: "5", placeholder: "Zip *" %>
            </div>

						<div class="event_time_zone js-event_time_zone hide clearfix">
							<div class="relative">
								<select name="ticketed_event[timezone]" class="can_select can_select_gray js-time_zone_select hundred-50 can_selectFullInner mr10 left">
				          <option  value="">Time Zone *</option>
				          <%= options_for_select(TIMEZONES, @ticketed_event.timezone) %>
								</select>
								<a class="tooltip js-tooltip left ml5">?<span>Select the timezone in which your event will take place.</span></a>
							</div>
						</div>

            <div class="event_contact clearfix">
                <%= f.text_field :contact_info, class: "create_action-editable js-create_action-editable", id: "event-contact", size: "35", placeholder: "Host Contact Information (Optional)" %>
                <a class="tooltip js-tooltip left ml5">?<span>Phone number or email address. Will be made public.</span></a>
            </div>
        </div>
        
        <div class="clearfix mb40">
            <a href="#upload_logo_modal" class="button upload_logo_button fancylink mb20 js-fancylink"><%if @ticketed_event.photo.exists?%>Edit<%else%>Add<%end%> Banner Image (optional)</a>
            <div class="hide">
                <div id="upload_logo_modal" class="centertext js-upload_logo_modal">
	                <p <%if !@ticketed_event.photo.exists?%>style="display:none;"<%end%>><a href="javascript:void(0);" class="image_delete_link js-image_delete_link">Click to remove the existing image</a> or...</p>
                    <p>Upload an image or banner for this action! It should be 1500px wide by any height (600px is suggested). It will be scaled to a final size of 750px wide.
                    </p>
                    <%= f.file_field :photo,:class => "mt10 action_photo_upload js-action_photo_upload" %>
                    <a href="javascript:;" onclick="$.fancybox.close();" class="button button_blue fullwidthbutton mt30">Okay!</a>
                </div>
            </div>
            <%= f.text_area :description_text, class: "redactor create_action-editable required js-create_action-editable js-redactor", id: "fundraiser_description", rows: 5, placeholder: "Ticketed event description *" %>
            <a class="tooltip js-tooltip lefti ml15">?<span>This is shown to people before they buy tickets. Let them know what the event will be like and why they should attend.</span></a>
        </div>
        
        
        <div id="select_tickets" class="white_box p20 border-box clearfix js-select_tickets">
            <h3 class="tear_off_title20 centertext nomb">Ticket Types <a class="tooltip js-tooltip ml5">?<span>You can have various ticket types costing different amounts that activists can purchase, with different amounts available before they sell out.</span></a></h3>
            <ul class="nomb lined">
	            <% tickets_index = 0 %>
	            <% if @ticketed_event.tickets.not_hidden.length == 0 %>
	            <li class="<%= tickets_index %>">
                      <div class="clearfix">
                          <div class="twentyfive left">
	                          <input class="required digits" id="ticketed_event_tickets_attributes_<%=tickets_index%>_total" min="1" name="ticketed_event[tickets_attributes][<%=tickets_index%>][total]" placeholder="# Available *" size="" step="1" type="number" value="">
                          </div>
                          <div class="fifty left">
                              <input class="ticket_title required" id="ticketed_event_tickets_attributes_<%=tickets_index%>_title" name="ticketed_event[tickets_attributes][<%=tickets_index%>][title]" placeholder="Ticket name (ex: VIP) *" size="30" type="text" value="">
                          </div>
                          <div class="twentyfive left">
                              <input class="required number" id="ticketed_event_tickets_attributes_<%=tickets_index%>_price" min="1" name="ticketed_event[tickets_attributes][<%=tickets_index%>][price]" placeholder="Cost in USD *" size="" step="1" type="text" value="">
                          </div>
                      </div>
                      <div class="clearfix mt10">
                          <input class="ticket_desc" id="ticketed_event_tickets_attributes_<%=tickets_index%>_description" name="ticketed_event[tickets_attributes][<%=tickets_index%>][description]" placeholder="Ticket description (optional)" size="30" type="text" value="">
                      </div>
                  </li>
                  <% tickets_index = tickets_index + 1 %>
                <% else %>
              	  <%= f.fields_for :tickets, @ticketed_event.tickets.not_hidden do |ticket_fields| %>
                  <li class="<%= tickets_index %>">
                      <div class="clearfix">
                          <div class="twentyfive left">
                              <%= ticket_fields.number_field(:total, min: 1, step: 1, size: "", placeholder: "# Available *", class: "required digits") %>
                          </div>
                          <div class="fifty left">
                              <%= ticket_fields.text_field(:title, placeholder: "Ticket name (ex: VIP) *", class: "ticket_title required") %>
                          </div>
                          <div class="twentyfive left">
                              <%= ticket_fields.text_field(:price, value: number_with_precision(ticket_fields.object.price, precision: 2), min: 1, step: 1, size: "", placeholder: "Cost in USD *", class: "required number") %>
                          </div>
                      </div>
                      <div class="clearfix mt10">
                          <%= ticket_fields.text_field(:description, placeholder: "Ticket description (optional)", class: "ticket_desc") %>
                      </div>
					  <%= ticket_fields.hidden_field(:id, class: "js-ticket_options_id") %>
                      
                      <% if tickets_index > 0 %>
                      <a href="javascript:void(0);" class="button mini_button righti mt5 js-remove_ticket_option remove_ticket_option" data-index="<%= tickets_index %>"><i class="fa fa-times"></i> Remove</a>
                      <% end %>
                  </li>
                  <% tickets_index = tickets_index + 1 %>
                  <% end %>
                <% end %>
                
              
                <li class="nopb centertext js-add_ticket_option_wrap">
                    <a href="javascript:void(0);" class="button button_blue js-add_ticket_option"><i class="fa fa-plus"></i> Add Another</a>
                </li>
            </ul>     
        </div>

        <div id="donation_recipient_wrap" class="clearfix">                       
            <h5>Your payment will go to</h5>
            
            <div class="donation_recipients clearfix">

                <div class="single_recipient please_add_recipient clearfix js-single_recipient js-please_add_recipient">
                    Please Select A Recipient
                </div>

                <% if @ticketed_event.recipient.present? %>
                  <div class="single_recipient single_recipient-create clearfix js-single_recipient js-single_recipient-create">
                      <a href="#" class="recipient_avatar_link">
                          <img src="<%= @ticketed_event.recipient.avatar %>" alt="Username" class="avatar donation_avatar" />
                      </a>
                      <div class="donation_info js-donation_info">
                          <div class="donation_info_user clearfix"><a href="#"><%= @ticketed_event.recipient.name %></a></div>
                          <% if @ticketed_event.recipient.city %><div class="donation_info_location clearfix"><%= "#{@ticketed_event.recipient.city}, #{@ticketed_event.recipient.state}" %></div><% end %>
                      </div>
                      <a class="remove_recipient js-remove_recipient button button_red" href="javascript:void(0);">Remove Recipient</a>
                      <input type="hidden" name="ticketed_event[recipient_id]" value="<%= @ticketed_event.recipient.id %>" />
                  </div>
                <% end %>

                <a href="#add_donation_recipient-modal" id="add_donation_recipient-link" class="button button_green inlineblock fancylink js-fancylink js-add_donation_recipient-link">Add Recipient</a>
                <a class="tooltip js-tooltip ml10">?<span>You can raise money for yourself, groups you are a member of, or any other individual or group on Action Network that is set up to receive payments. You can have one, and only one, recipient who will receive money when activists buy tickets.</span></a>
                
                <div class="hide">
                    <div id="add_donation_recipient-modal">
                        <h4 class="entry-title mb20 pb10 centertext js-entry-title">Raise For Yourself Or Your Groups<a class="tooltip js-tooltip ml10">?<span>Before you can take contributions for yourself or your groups you must set up a fundraising account -- we'll walk you through it next if this is your first time. If you don't see a group you're a member of in this list, it means that group isn't set up to take contributions yet. Ask a group administrator to set up the first fundraising page so they can set up the group's fundraising account.</span></a></h4>
                        <div class="relative clearfix">
			              <select name="donation_groups" id="donation_groups" class="donation_groups_select js-donation_groups"> 
			                <option value="<%=current_user.permalink%>" data-link="https://<%=ENV['DOMAIN']%>/users/<%=current_user.permalink%>/profile" data-avatar="<%=current_user.avatar%>" data-name="<%=current_user.name%>" data-location="<%="#{current_user.city}, #{current_user.state}"if current_user.city%>" data-id="user_<%=current_user.id
			                %>">Yourself</option>
			                <optgroup label="Your Groups">
			                  <%for group in (current_user.led_groups.not_hidden(current_user).not_network.find_all_by_status(2)+current_user.unled_groups.not_hidden(current_user).find_all_by_status(2,:conditions => ["wepay_access_token is not null"])).sort_by{|gp| gp.admin_title.downcase}%>
			                    <option value="<%=group.permalink%>" data-link="https://<%=ENV['DOMAIN']%>/groups/<%=group.permalink%>" data-avatar="<%=group.photo.url %>" data-name="<%=group.title%>" data-location="<%="#{group.city}, #{group.state}"if group.city%>" data-id="group_<%=group.id%>" ><%=group.admin_title%></option>
			                  <%end%>
			                </optgroup>
			
			                <% network_groups = current_user.network_groups || [] %>
			
			                <% if !network_groups.empty? %>
			                  <optgroup label="<%=current_user.network(true).name%> Network">
			                    <%for group in network_groups %>
			                      <option value="<%=group.permalink%>" data-link="https://<%=ENV['DOMAIN']%>/groups/<%=group.permalink%>" data-avatar="<%=group.photo.url %>" data-name="<%=group.title%>" data-location="<%="#{group.city}, #{group.state}"if group.city%>" data-id="group_<%=group.id%>" class="spacer-<%= group.relative_network_level.to_s%> dropspacer"><%= (group.admin_title).html_safe %></option>
			                    <%end%>
			                  </optgroup>
			                <% end %>
			              </select>
			              
			              <a href="javascript:;" class="button button_blue choose_recipient_button js-choose_recipient_button donation_groups_button js-donation_groups_button right">Add</a>
			            </div>
                        
                        <div id="or_search_donation">Or</div>
                        
                        <h4 class="entry-title js-entry-title mb20 pb10 centertext">Raise For Others On Action Network Who Are Set Up To Take Donations</h4>
            
			            <input type="text" name="donation_user_search" id="donation_user_search" class="left js-donation_user_search" placeholder="User or Group Name"/>
			            <input type="submit" class="right button button_green donation_user_search_button js-donation_user_search_button" value="Search" />
			            <div class="clear"></div>
			            <div id="search_recipient_list" class="js-search_recipient_list">
			              Search above for individuals or groups to raise for.
			            </div>
                    </div>
                </div>
            </div>
            
        </div>
        
        <div id="bypass_email_wrap" class="clearfix hide js-bypass_email_wrap">
	        <label class="nofloat">
	            <%= f.check_box :donations_without_email %> Make email address optional for contributors.<a class="tooltip js-tooltip ml10">?<span>Contributors without an email address will be assigned a random one by the system and unsubscribed from mailings.</span></a>
	        </label>
	    </div>
        
        <div id="donate_login" class="disabled">
          Saved your payment information with Action Network? <a href="javascript:void(0);">Log in now</a>.
        </div>
        
        <div id="donate_cc_form" class="clearfix">
            <ul class="clearfix disabled nofloat">
                <li>
                    <label>First Name *</label>
                    <input type="text" size="35" disabled class="required" name="form-first_name" id="form-first_name" />
                </li>
                <li>
                    <label>Last Name *</label>
                    <input type="text" size="35" disabled class="required" name="form-last_name" id="form-last_name" />
                </li>
                <li>
                    <label>Email *</label>
                    <input type="email" size="35" disabled class="required" name="form-email" id="form-email" />
                </li>
                <li>
                    <label>Billing Address *</label>
                    <input type="text" size="35" disabled class="required" name="form-address" id="form-address" />
                </li>
                <li>
                    <label>City *</label>
                    <input type="text" size="35" disabled class="required" name="form-city" id="form-city" />
                </li>
                <li class="cc_li_half js-cc_li_half">
                    <label>State *</label>
                    <span class="donate_state">
                        <select name="form-state" id="form-state" disabled class="can_select"> 
                            <option value=""></option> 
                            <option value="AL">Alabama</option>
                            <option value="AK">Alaska</option> 
                            <option value="AZ">Arizona</option> 
                            <option value="AR">Arkansas</option> 
                            <option value="CA">California</option> 
                            <option value="CO">Colorado</option> 
                            <option value="CT">Connecticut</option> 
                            <option value="DE">Delaware</option> 
                            <option value="DC">District Of Columbia</option> 
                            <option value="FL">Florida</option> 
                            <option value="GA">Georgia</option> 
                            <option value="HI">Hawaii</option> 
                            <option value="ID">Idaho</option> 
                            <option value="IL">Illinois</option> 
                            <option value="IN">Indiana</option> 
